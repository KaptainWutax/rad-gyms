{
	"schemaVersion": 1,
	"id": "rad-gyms",
	"version": "${version}",
	"name": "Rad Gyms",
	"description": "Adds roguelike gyms in Cobblemon. RCT Api based mod.",
	"authors": [
		"Gitoido"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/rad-gyms/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "lol.gito.radgyms.RadGyms::init",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "lol.gito.radgyms.RadGymsClient::init",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "lol.gito.radgyms.datagen.RadGymsDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"rad-gyms.mixins.json"
	],
	"depends": {
		"java": ">=21",
		"minecraft": "~1.21.1",
		"fabricloader": ">=0.16.14",
		"fabric": "*",
		"fabric-api": "*",
		"fabric-language-kotlin": "*",
		"architectury": ">=13.0.8",
		"rctapi": ">=0.16.0-beta",
		"cobblemon": ">=1.6.1+1.21.1",
		"admiral": "0.4.7+1.21.1+fabric"
	},
	"custom": {
		"mc-publish": {
			"modrinth": "eF8kqlHd",
			"curseforge": "1196570"
		}
	}
}
